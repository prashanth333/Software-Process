        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	// Add your code here
	
	
	if( current.state == '230' && current.u_on_hold.changesFrom(true)){
				current.u_follow_up_complete = 'true';		
	}
else if(current.state == '430' && current.state.changesFrom(230))
	{
		current.u_follow_up_complete = 'false';
	}
	else if (current.state == '430' && current.state.changesFrom(450))
		{
			current.u_follow_up_complete = 'true';
		}
})(current, previous);]]></script>
